        <stringProp name="cacheKey">false</stringProp>
        <stringProp name="script">import org.apache.jmeter.config.Arguments;
import org.apache.jmeter.config.Argument;
import org.apache.jmeter.protocol.http.control.HeaderManager;
import org.apache.jmeter.protocol.http.control.CookieManager;

boolean FAILED_LOGGING_ENABLED = vars.get(&quot;FAILED_LOGGING_ENABLED&quot;) == &quot;true&quot;;
boolean SHOULD_LOG = !prev.isSuccessful() &amp;&amp; FAILED_LOGGING_ENABLED;

if (SHOULD_LOG) {
	String journeyName = ctx.getThreadGroup().getName();
	String method = sampler.getMethod();
	String url = sampler.getUrl().toString()
	log.error(&quot;${journeyName}: ${method} ${url} failed&quot;)
	Arguments arguments = sampler.arguments;
	if (arguments.getArgumentCount() &gt; 0) {
		log.error(&quot;Data passed: &quot; + arguments.getArgument(0).getValue())
	}

	HeaderManager headerManager = sampler.getHeaderManager();
	if (headerManager != null &amp;&amp; headerManager.size() &gt; 0) {
		log.error(&quot;Headers:&quot;);
		int size = headerManager.size();
		for (int i = 0; i &lt; size; i++) {
			String name = headerManager.get(i).getName();
			String value = headerManager.get(i).getValue();
			log.error(&quot;${name}=${value}&quot;);
		}
	}

	CookieManager cookieManager = sampler.getCookieManager();
	if (cookieManager != null &amp;&amp; cookieManager.getCookieCount() &gt; 0) {
		log.error(&quot;Cookies:&quot;);
		int size = cookieManager.getCookieCount();
		for (int i = 0; i &lt; size; i++) {
			String name = cookieManager.get(i).getName();
			String value = cookieManager.get(i).getValue();
			log.error(&quot;${name}=${value}&quot;);
		}
	}
}</stringProp>
