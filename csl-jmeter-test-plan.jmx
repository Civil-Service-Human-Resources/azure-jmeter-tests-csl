      <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="bsPostProcessor" enabled="false">
        <stringProp name="filename"></stringProp>
        <stringProp name="parameters"></stringProp>
        <boolProp name="resetInterpreter">false</boolProp>
        <stringProp name="script">import java.text.DecimalFormat;
import java.text.DateFormat;
import java.text.SimpleDateFormat;

String journeyName = ctx.getThreadGroup().getName();

if (journeyName.equals(&quot;tg_SetUp&quot;)){
	return;
}

int numThreads = ctx.getThreadGroup().getNumThreads();
int thrNumber = ctx.getThreadNum() + 1;

String stepName = ctx.getCurrentSampler().getName();

String startTime = ctx.getThread().getStartTime().toString();
String endTime = ctx.getThread().getEndTime().toString();

String respCode = ctx.getPreviousResult().getResponseCode();
String respMsg = ctx.getPreviousResult().getResponseMessage();

String responseTime = ctx.getPreviousResult().getTime().toString();

Date dt = new Date();
SimpleDateFormat df = new SimpleDateFormat(&quot;yyyy-MM-dd HH:mm:ss.SSS&quot;);
dt.setDate(dt.getDate());
String fmtDate = df.format(dt);

boolean isSuccessful = prev.isSuccessful();

// passed and failed calculation
int passed = Integer.parseInt(props.get(&quot;PASSED&quot;)) + 1;
int failed = Integer.parseInt(props.get(&quot;FAILED&quot;)) + 1;
long totalRequests = Long.parseLong(props.get(&quot;TOTAL_REQUESTS&quot;)) + 1;

double newResponseTime = Double.parseDouble(responseTime);
double responseTimeTotal = Double.parseDouble(props.get(&quot;RESPONSE_TIME&quot;)) + newResponseTime;
double maxResponseTime = Double.parseDouble(props.get(&quot;MAX_RESPONSE_TIME&quot;));

props.put(&quot;TOTAL_REQUESTS&quot;, totalRequests.toString());
props.put(&quot;RESPONSE_TIME&quot;, responseTimeTotal.toString());

if ( maxResponseTime &lt; newResponseTime){
	props.put(&quot;MAX_RESPONSE_TIME&quot;, newResponseTime.toString());
}

if (isSuccessful){
	props.put(&quot;PASSED&quot;, passed.toString());
}else{
  	props.put(&quot;FAILED&quot;, failed.toString());
}

if (!Boolean.parseBoolean(props.get(&quot;customLogOutputEnabled&quot;))){
	return;
}

//responseTimeTotal.toString()

log.info(fmtDate + &quot;, journey: &quot; + journeyName + &quot;, step: &quot; + stepName + &quot;, Thread: &quot; + thrNumber + &quot;, startedAt: &quot; + startTime + &quot;, endedAt: &quot; + endTime + &quot;, status: &quot; + respCode + &quot;, responseTime: &quot; + responseTime);

</stringProp>
