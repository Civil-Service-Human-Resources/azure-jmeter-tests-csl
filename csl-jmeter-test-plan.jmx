              <stringProp name="script">import java.time.LocalDate
import java.time.temporal.ChronoUnit

int duration = Integer.parseInt(vars.get(&quot;report_duration&quot;));

LocalDate reportEnd = LocalDate.now();
LocalDate reportStart = reportEnd.minus(duration, ChronoUnit.DAYS);

vars.put(&quot;startDay&quot;, reportStart.getDayOfMonth().toString());
vars.put(&quot;startMonth&quot;, reportStart.getMonthValue().toString());
vars.put(&quot;startYear&quot;, reportStart.getYear().toString());

vars.put(&quot;endDay&quot;, reportEnd.getDayOfMonth().toString());
vars.put(&quot;endMonth&quot;, reportEnd.getMonthValue().toString());
vars.put(&quot;endYear&quot;, reportEnd.getYear().toString());</stringProp>
