            <stringProp name="TestPlan.comments">DB setup for initial data state pre-test - currently just resetting new profile users</stringProp>
            <stringProp name="dataSource">dbSetup</stringProp>
            <stringProp name="query">SET SQL_SAFE_UPDATES = 0;

-- Delete interest entries from civil servants

DELETE FROM csrs.civil_servant_interests WHERE civil_servant_id IN (SELECT csrs.civil_servant.id FROM csrs.civil_servant
	JOIN csrs.identity ON csrs.identity.id = csrs.civil_servant.identity_id
    JOIN identity.identity ON identity.identity.uid = csrs.identity.uid
    WHERE email LIKE &apos;user%@example.com&apos;);

-- Delete other areas of work entries from civil servants
    
DELETE FROM csrs.civil_servant_other_areas_of_work WHERE civil_servant_id IN (SELECT csrs.civil_servant.id FROM csrs.civil_servant
	JOIN csrs.identity ON csrs.identity.id = csrs.civil_servant.identity_id
    JOIN identity.identity ON identity.identity.uid = csrs.identity.uid
    WHERE email LIKE &apos;user%@example.com&apos;);

-- Delete civil servant records

DELETE FROM  csrs.civil_servant 
	WHERE identity_id IN (SELECT csrs.identity.id FROM csrs.identity JOIN identity.identity ON identity.identity.uid = csrs.identity.uid WHERE email LIKE &apos;user%@example.com&apos;);

-- Delete CSRS Identity entries

DELETE FROM csrs.identity WHERE uid IN (
SELECT uid FROM identity.identity WHERE email LIKE &apos;user%@example.com&apos;);


-- Ensure line manager identity is active and unlocked

UPDATE 
	identity.identity 
SET 
	active = 1,
    locked = 0
WHERE 
	email = &apos;lc687@test.com&apos;;</stringProp>
