double totalResponsTime = Double.parseDouble(props.get(&quot;RESPONSE_TIME&quot;));
double maxResponsTime = Double.parseDouble(props.get(&quot;MAX_RESPONSE_TIME&quot;));

String avgResponseTime = (Math.round( (totalResponsTime * 1.0) / total_requests )).toString();
String perc_passed = (Math.round( (passed * 1.0) / total_requests * 100.0)).toString();
String perc_failed = (Math.round( (failed * 1.0) / total_requests * 100.0)).toString();

log.info(&quot;---------------------------------------------------------------&quot;);
