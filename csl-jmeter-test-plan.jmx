      <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="bsSummary" enabled="true">
        <boolProp name="resetInterpreter">false</boolProp>
        <stringProp name="parameters"></stringProp>
        <stringProp name="filename"></stringProp>
        <stringProp name="script">int passed = Integer.parseInt(vars.get(&quot;PASSED&quot;));
int failed = Integer.parseInt(vars.get(&quot;FAILED&quot;));

int total_requests = passed + failed;

String perc_passed = (passed / total_requests * 100).toString();
String perc_failed = (failed / total_requests * 100).toString();


log.info(&quot;PASSED: &quot; + passed.toString() + &quot; [ &quot; + perc_passed + &quot;% ] , FAILED: &quot; + failed.toString() + &quot; [ &quot; + perc_failed + &quot;% ] &quot;);</stringProp>
