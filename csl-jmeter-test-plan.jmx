        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="bsGenerateSummary" enabled="true">
          <stringProp name="BeanShellSampler.query">String journeyName = ctx.getThreadGroup().getName();

if (journeyName.equals(&quot;tg_SetUp&quot;)){
	return;
}

int passed = Integer.parseInt(props.get(&quot;PASSED&quot;));
int failed = Integer.parseInt(props.get(&quot;FAILED&quot;));
long total_requests = Long.parseLong(props.get(&quot;TOTAL_REQUESTS&quot;));
double totalResponsTime = Double.parseDouble(props.get(&quot;RESPONSE_TIME&quot;));
double maxResponsTime = Double.parseDouble(props.get(&quot;MAX_RESPONSE_TIME&quot;));

String avgResponseTime = (Math.round( (totalResponsTime * 1.0) / total_requests )).toString();
String perc_passed = (Math.round( (passed * 1.0) / total_requests * 100.0)).toString();
String perc_failed = (Math.round( (failed * 1.0) / total_requests * 100.0)).toString();

log.info(&quot;---------------------------------------------------------------&quot;);
log.info(&quot;                   S U M M A R Y  R E P O R T                  &quot;);
log.info(&quot;---------------------------------------------------------------&quot;);
log.info(&quot;&quot;);
log.info(&quot;TOT.REQ       : &quot; + total_requests);
log.info(&quot;PASSED        : &quot; + passed.toString() + &quot; [ &quot; + perc_passed + &quot;% ]&quot;);
log.info(&quot;FAILED        : &quot; + failed.toString() + &quot; [ &quot; + perc_failed + &quot;% ]&quot;);
log.info(&quot;MAX RPT       : &quot; + maxResponsTime + &quot; ms&quot;);
log.info(&quot;AVG RPT       : &quot; + avgResponseTime + &quot; ms&quot;);
log.info(&quot;&quot;);
log.info(&quot;--------------------- END OF TEST SUMMARY ---------------------&quot;);
